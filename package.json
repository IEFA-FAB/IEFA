{
  "name": "iefa",
  "private": true,
  "packageManager": "bun@1.3.6",
  "overrides": {
    "react": "^19.2",
    "react-dom": "^19.2",
    "@tanstack/router-core": "^1.139.6",
    "@tanstack/query-core": "^5.66.5",
    "vite": "npm:rolldown-vite@latest"
  },
  "scripts": {
    "dev": "bun run --filter './apps/**' dev",
    "build": "bun run --filter './apps/**' build",
    "typecheck": "bun run --filter '*' typecheck",
    "sisub:dev": "bun --cwd ./apps/sisub dev",
    "iefa:dev": "bun --cwd ./apps/iefa dev",
    "api:dev": "bun --cwd ./apps/api dev",
    "docs:dev": "bun --cwd ./apps/docs dev",
    "format": "biome format --write .",
    "lint": "biome check --write .",
    "check": "biome check . && bun run typecheck",
    "fix": "biome check --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
